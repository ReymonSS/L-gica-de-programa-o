import java.util.Scanner;
public class extremamente_basico {
	public static void main ( String []args ) {
		int A;
		int B;
		int x;
		Scanner in = new Scanner( System.in );
		System.out.println("Informe o valor da variavel A \n");
		A = in.nextInt();
		System.out.println("Informe o valor da variavel A \n");
		B = in.nextInt();
		x = A + B;
		System.out.println("X = " +x );
	}
}
